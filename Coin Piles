#include<bits/stdc++.h>
#define ll long long int
using namespace std;
 
int main()
{
   int t;
   cin>>t;
   while(t--)
   {
       ll x,y;
       cin>>x>>y;
       ll c=0;
       if(x<=y)
        c++;
       ll sum=(x+y)%3;
       if((sum==0 && y<=x*2 && c==1) || (sum==0 && x<=y*2 && c==0))
        cout<<"YES"<<endl;
       else
        cout<<"NO"<<endl;
   }
return 0;
}
