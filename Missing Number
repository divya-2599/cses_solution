#include<bits/stdc++.h>
#define ll long long int
using namespace std;
int main()
{
    int n;
    cin>>n;
    int a[n],b[n+1]={0};
    for(int i=0;i<n-1;i++)
    {
       cin>>a[i];
       b[a[i]]++;
    }
    for(int i=1;i<=n;i++)
    {
            if(b[i]==0)
    {
      cout<<i;
      break;
    }
    }
return 0;
}
