#include<bits/stdc++.h>
#define ll long long int
using namespace std;
ll power(int x,int y,int mod)
{
    if(y==0)
        return 1;
    else if(y%2==0)
    {
        ll z=power(x,y/2,mod);
        return (z*z)%mod;
    }
    else
        return ((x%mod)*(power(x,y-1,mod)))%mod;
}
int main()
{
int t;
cin>>t;
while(t--)
{
    int x,y,n;
    cin>>x>>y>>n;
    ll z=power(y,n,1000000007-1);
    cout<<power(x,z,1000000007)<<endl;
}
return 0;
}
